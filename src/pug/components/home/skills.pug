article.skills#skills-part
	+lazyPicture({
		src: `${baseDir}images/sprites/svg/lineMain2.svg`,
		alt: 'line main 2',
		appear: false,
	}, [
			{
				srcset: `${baseDir}images/sprites/svg/lineMain2m.svg`,
				media: '(min-width: 0px) and (max-width: 425px)',
			},
			{
				srcset: `${baseDir}images/sprites/svg/lineMain2.svg`,
				media: '(min-width: 426px)',
			}
		]
	).container-lines__line--2.picture
	.skills-container
		h2.skills__title.title--visible.hidden-right
			!= data.home.skills.title
		ul.skills__list
			each item, key in data.home.skills.list
				li.list__item.block-text--visible.hidden-up
					h4.list__title
						!= item.name
					p.list__text
						!= item.items